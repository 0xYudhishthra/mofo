# ASI Service Configuration
ASI_PROXY_PORT=4000
ASI_WS_PORT=4001
NODE_ENV=development

# MOFO Integration Points
MOFO_APP_URL=http://localhost:3002
BOOTH_BACKEND_URL=http://localhost:3004
BOOTH_FRONTEND_URL=http://localhost:3003
RELAYER_WS_URL=ws://localhost:8765

# Fetch.ai Agentverse
AGENTVERSE_ENDPOINT=https://agentverse.ai
AGENTVERSE_API_KEY=your_agentverse_api_key


# uAgent Configuration
UAGENT_SEED=mofo-asi-agent-seed
UAGENT_PORT=8000
UAGENT_ENDPOINT=http://localhost:8000

# ASI LLM
ASILLM_ENDPOINT=https://api.fetch.ai/llm
ASILLM_MODEL=asi-7b
ASILLM_API_KEY=your_asillm_api_key

# Redis Configuration (REQUIRED for queue management)
REDIS_HOST=localhost
REDIS_PORT=6379
REDIS_PASSWORD=

# Twitter Integration (Choose ONE approach)
# Option 1: Direct Twitter API v2
TWITTER_API_KEY=your_twitter_api_key
TWITTER_API_SECRET=your_twitter_api_secret
TWITTER_BEARER_TOKEN=your_twitter_bearer_token
TWITTER_ACCESS_TOKEN=your_twitter_access_token
TWITTER_ACCESS_SECRET=your_twitter_access_secret

# Option 2: MCP Server (if you have one running)
MCP_ENDPOINT=http://localhost:3100
MCP_API_KEY=your_mcp_api_key

# Agentverse Template Configuration
AGENTVERSE_TEMPLATE_ADDRESS=your_template_agent_address

# Feature Flags
FEATURE_ENHANCED_EEG=true
FEATURE_AUTONOMOUS_AGENTS=true
FEATURE_DECENTRALIZED_MATCHING=true
FEATURE_LLM_CONVERSATIONS=true
FEATURE_TWITTER_PERSONALITY=true